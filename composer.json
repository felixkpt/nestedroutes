{
    "name": "felixkpt/nestedroutes",
    "version": "1.0.1",
    "description": "A package for handling nested routes in PHP applications. This package provides a flexible and intuitive way to define and manage nested routes, with support for role-based permissions.",
    "type": "library",
    "keywords": [
        "routing",
        "nested",
        "php",
        "roles",
        "permissions"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Biwott",
            "email": "felixkpt@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-permission": ">=5.10"
    },
    "autoload": {
        "psr-4": {
            "Felixkpt\\Nestedroutes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Felixkpt\\Nestedroutes\\Providers\\NestedroutesServiceProvider"
            ]
        }
    }
    
}